  "repository": {
    "url": "https://github.com/margostino/babel-api"
  },
  "scripts": {
    "check:types": "node ./node_modules/typescript/bin/tsc --noEmit",
    "start:vercel": "nodemon --exec vercel dev",
    "start:dev": "NODE_ENV=development PORT_DEV_SERVER=9000 nodemon --exec ts-node --files src/runner/server.ts",
    "test": "yarn test:unit && yarn test:integration",
    "test:integration": "NODE_ENV=test jest --config jest.integration.js --passWithNoTests",
    "test:unit": "NODE_ENV=test jest --config jest.unit.js --passWithNoTests"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.8",
    "@vercel/node": "^3.1.7",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "junit-merge": "^2.0.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "prettier-2": "npm:prettier@^2",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "express-async-router": "^0.1.15",
